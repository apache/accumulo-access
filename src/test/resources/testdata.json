  {
    "description": "expressions with non alpha numeric characters",
    "auths": [
      [
        "a_b",
        "a-c",
        "a/d",
        "a:e",
        "a.f",
        "a_b-c/d:e.f"
      ]
    ],
    "tests": [
      {
        "expectedResult": "ACCESSIBLE",
        "expressions": [
          "a_b",
          "\"a_b\"",
          "a-c",
          "\"a-c\"",
          "a/d",
          "\"a/d\"",
          "a:e",
          "\"a:e\"",
          "a.f",
          "\"a.f\"",
          "a_b|a_z",
          "a-z|a-c",
          "a/d|a/z",
          "a:e|a:z",
          "a.z|a.f",
          "a_b&a-c&a/d&a:e&a.f",
          "(a-z|a-c)&(a/d|a/z)",
          "a_b-c/d:e.f",
          "a_b-c/d:e.f&a/d"
        ]
      },
      {
        "expectedResult": "INACCESSIBLE",
        "expressions": [
